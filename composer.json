{
    "name": "buibr/yii2-typeahead-widget-bs4",
    "description": "The bootstrap typeahead widget for the Yii framework and bootstrap4",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "widget",
        "typeahead"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/2amigos/yii2-type-ahead-widget",
    "authors": [
        {
            "name": "Alexander Kochetov",
            "email": "creocoder@gmail.com",
            "role": "Developer"
        },
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        },
        {
            "name": "Burhan Ibrahimi",
            "email": "burhan@wflux.pro",
            "homepage": "http://wflux.pro",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13",
        "yiisoft/yii2-bootstrap4": "^1.0@dev",
        "bower-asset/typeahead.js": "~0.11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "buibr\\typeahead\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
